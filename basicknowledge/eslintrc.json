{
    "extend": ["airbnb","prettier","plugin:node/recommend"],
    "plugins":["prettier"],
    "rules":{
        "prettier/prettier":"error",
        "spaced-comment":"off",
        "no-console":"warn",
        "consistent-return":"off",
        "func-names":"off",
        "object-shordhand":"off",
        "no-process-exit":"off",
        "no-param-reassign":"off",
        "no-return-await":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "prefer-destructuring":["error",{"object":true,"array":false}],
        "no-unsusued-vars":["off",{"argsIgnorePattern":"req|res|next|val"}]
    }
}